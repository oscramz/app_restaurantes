<h1>Formulario para nuevo tipo de comida</h1>

<%= form_with(model: @nuevo_tipo_comida, url: tipos_comidas_path) do |formulario| %>
    <%= formulario.label       :tipo %>
    <%= formulario.text_field :tipo %>
    <%= formulario.submit 'Crear' %>
<% end %>
<br>
<%= link_to 'Volver', tipos_comidas_path %>